<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TrakHound_Dashboard.Styles"
                    xmlns:ice="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">

    
    

    <RadialGradientBrush ice:Freeze="True" x:Key="Navigation_Menu_Background" GradientOrigin="0.5,0.5" Center="0.5,0.5" RadiusX="0.5" RadiusY="0.5">
        <RadialGradientBrush.GradientStops>
            <GradientStop Color="#40a0ff" Offset="0.3" />
            <GradientStop Color="#0080ff" Offset="1" />
        </RadialGradientBrush.GradientStops>
    </RadialGradientBrush>

</ResourceDictionary>